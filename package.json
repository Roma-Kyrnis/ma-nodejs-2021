{
  "name": "homework-01",
  "version": "1.0.0",
  "description": "Homework-01",
  "main": "src",
  "scripts": {
    "start": "node src",
    "seeds:create": "node ./src/db/seeds",
    "knex:migrate:make": "./node_modules/.bin/knex --knexfile=./src/db/knex/knexfile.js migrate:make",
    "knex:migrate:latest": "./node_modules/.bin/knex --knexfile=./src/db/knex/knexfile.js migrate:latest",
    "knex:migrate:up": "./node_modules/.bin/knex --knexfile=./src/db/knex/knexfile.js migrate:up",
    "knex:migrate:down": "./node_modules/.bin/knex --knexfile=./src/db/knex/knexfile.js migrate:down",
    "knex:migrate:rollback": "./node_modules/.bin/knex --knexfile=./src/db/knex/knexfile.js migrate:rollback",
    "pg:migrate:create": "./node_modules/.bin/node-pg-migrate --config-file=./src/db/pg/config-file.js --migrations-dir=./src/db/pg/migrations create",
    "pg:migrate:up": "./node_modules/.bin/node-pg-migrate --config-file=./src/db/pg/config-file.js --migrations-dir=./src/db/pg/migrations up",
    "pg:migrate:down": "./node_modules/.bin/node-pg-migrate --config-file=./src/db/pg/config-file.js --migrations-dir=./src/db/pg/migrations down",
    "pg:migrate:redo": "./node_modules/.bin/node-pg-migrate --config-file=./src/db/pg/config-file.js --migrations-dir=./src/db/pg/migrations redo"
  },
  "dependencies": {
    "@awaitjs/express": "^0.6.3",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "byline": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.15",
    "node-pg-migrate": "^5.9.0",
    "pg": "^8.5.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "prettier": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Roman-Kirnos/ma-nodejs-2021.git"
  },
